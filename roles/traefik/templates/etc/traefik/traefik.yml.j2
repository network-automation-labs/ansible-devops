#jinja2:lstrip_blocks: True
---
entryPoints:
  http:
    address: ':80'
  https:
    address: ':443'
  traefik:
    address: ':8080'

api:
  dashboard: true
  insecure: true

providers:
  file:
    directory: "/etc/traefik/conf.d"
{% if traefik_docker_enabled | default(false) %}
  docker:
    endpoint: "unix:///var/run/docker.sock"
    watch: true
    exposedbydefault: false
{% endif %}
